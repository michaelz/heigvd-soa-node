
    // Logic login stuff
    $$("login").attachEvent("onItemClick", function (){
        $.post( "/auth", {
             username: $$("username").getValue(),
             password: $$("password").getValue()
          }).done(function( data ) {
            token = data.data;
            // Store the token :
            localStorage.setItem('woa-token',token);
        });
    });
